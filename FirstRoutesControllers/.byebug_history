  WHERE (actor.name = 'Ben Affleck' AND casting.ord != 1);
  JOIN actor all_actors on all_actors.id = all_casting.actorid
  JOIN casting all_casting on all_casting.movieid = movie.id
  JOIN movie on movie.id = casting.movieid
  JOIN casting on actor.id = casting.actorid
  FROM actor
  SELECT DISTINCT movie.title, all_actors.name
